Input: a[] = [1, 2, 5, 4, 0], b[] = [2, 4, 5, 0, 1]
Output: true
Explanation: Both the array can be rearranged to [0,1,2,4,5]



class Solution {
  public:
    bool checkEqual(vector<int>& a, vector<int>& b) {
        // code here
         if (a.size() != b.size()) return false;
    
        sort(a.begin(),a.end());
        sort(b.begin(),b.end());
        
        return a==b;
    }
};
