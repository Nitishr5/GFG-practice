bool search(Node* root, int x) {
    // base case
    
    if(root==NULL){
        return false;
    }
    
    if(root->data==x)
        return true;
        
        if(root->data>x)
        {
            //left part me dekho
            return search(root->left,x);
        }
        else
        {
            // right side me dekho
            return search(root->right,x);
        }
}
