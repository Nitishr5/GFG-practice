Input: s = "Geeks"
Output: "skeeG"
Input: s = "for"
Output: "rof"




// User function Template for C++
class Solution {
  public:
    string reverseString(string& s) {
        // code here
        int n=s.size();
        int st=0,end=n-1;
        
        while(st<end){
            swap(s[st],s[end]);
            st++;
            end--;
        }
        return s;
    }
};
